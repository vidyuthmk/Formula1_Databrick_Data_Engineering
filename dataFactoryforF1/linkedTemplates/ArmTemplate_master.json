{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dataFactoryforF1"
		},
		"ls_formula1_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_formula1_adls'"
		},
		"ls_formula1_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1newdatalake.dfs.core.windows.net/"
		},
		"tr_proccess_fomula1_data_properties_pipeline_parameters_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dataFactoryforF1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_formula1_adls_accountKey": {
						"value": "[parameters('ls_formula1_adls_accountKey')]"
					},
					"ls_formula1_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_formula1_adls_properties_typeProperties_url')]"
					},
					"tr_proccess_fomula1_data_properties_pipeline_parameters_window_end_date": {
						"value": "[parameters('tr_proccess_fomula1_data_properties_pipeline_parameters_window_end_date')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}